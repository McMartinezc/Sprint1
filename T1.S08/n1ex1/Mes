package n1ex1;

import java.util.Objects;

public class Mes {

	// Declaració atributs
	private String name;

	// Metode constructor
	public Mes(String name) {
		this.name = name;
	}

	// Metodes GETTERS i SETTERS
	public String getName() { // Donem informació
		return name;
	}

	public void setName(String name) {// Canvi d'informació
		this.name = name;
	}


	public String toString() {
		return name;
	}

}
