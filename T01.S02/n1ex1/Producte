package n1ex1;

public class Producte {

	// Declarem atributs
	private String nom;
	private float preu;

	// Metode constructor
	public Producte(String nom, float preu) {
		this.nom = nom;
		this.preu = preu;
	}

	// Metodes GETTERS i SETTERS
	public String getNom() {
		return nom;
	}

	public double getPreu() {
		return preu;
	}

}
