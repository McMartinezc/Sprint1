class CalculoDniTest {
	String DNI = "TRWAGMYFPDXBNJZSQVHLCKE"; // Lletres del DNI en string
	
	@BeforeEach
	void setUp() throws Exception {
	}

	@AfterEach
	void tearDown() throws Exception {
	}

	@Test
	public int testCalculRestoDNI(String DNI) {
		return Integer.parseInt(DNI.substring(0), DNI.length()-1);
		
	}
	
	@Test
	public int validarDNI(String DNI) {
		return Integer.parseInt(DNI.substring(0), DNI.length()-1);
		
	}
}
