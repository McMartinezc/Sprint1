package n1ex2;

import java.util.Scanner;

public class CalculoDni {

	
	// Calcul resto del numDNI introdu√Øt
	public char calculRestoDNI(int numDNI) {
		String DNI = "TRWAGMYFPDXBNJZSQVHLCKE"; // Lletres del DNI en string de tipus constant
		int resto = numDNI % 23;
		return DNI.charAt(resto);
	}

}
