package n2ex2;

import java.util.ArrayList;

public class NoGenericMethods <T> {
	
	//Declaraci√≥ de variables
	private T nom;
	private T cognom;
	private T edat;
	
	
	//Metode constructor
	public NoGenericMethods(T dada, T dada2, T valor) {
		nom = dada;
		cognom= dada2;
		edat = valor;
		
	}
	
	//Metodes GETTER i SETTER
	public T getNom() {
		return nom;
	}

	public void setNom(T dada) {
		nom = dada;
	}

	public T getCognom() {
		return cognom;
	}

	public void setCognom(T dada2) {
		cognom = dada2;
	}

	public T getEdat() {
		return edat;
	}

	public void setEdat(T valor) {
		edat = valor;
	}
}
	
	//Metode propi de la classe generic per printar les dades
	public  ArrayList<T>llista  imprimirDades (T dada, T dada2, T valor) {
		
		return llista;
		
	
	}
	
}
