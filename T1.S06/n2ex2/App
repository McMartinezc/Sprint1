package n2ex2;

public class App {
	
	public static void main(String[] args) {
		
	
	GenericArray <Persona> llistaGenerica = new GenericArray <Persona>();
	llistaGenerica.afegirElement(new Persona ("MC","MC",37));
	llistaGenerica.imprimirArray();
	
	GenericArray<Integer> llistaGenericaNums= new GenericArray<Integer>();
	llistaGenericaNums.afegirElement(8);
	llistaGenericaNums.imprimirArray();
	
	GenericArray<String> llistaGenericaString = new GenericArray<String>();
	llistaGenericaString.afegirElement("Cadena de paraules");
	llistaGenericaString.imprimirArray();
	
	}
	
}
