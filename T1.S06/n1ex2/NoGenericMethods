package n1ex2;


public class NoGenericMethods <T> { //T,U,K declaración para clases genericas
	
	//Declaració de variables
	private T nom;
	private T cognom;
	private T edat;
	
	//Metode constructor
	public NoGenericMethods(T nom, T cognom, T edat) {
		this.nom = nom;
		this.cognom= cognom;
		this.edat = edat;
	}
	
	//Metodes GETTER i SETTER
	public T getNom() {
		return nom;
	}

	public void setNom(T nom) {
		this.nom = nom;
	}

	public T getCognom() {
		return cognom;
	}

	public void setCognom(T cognom) {
		this.cognom = cognom;
	}

	public T getEdat() {
		return edat;
	}

	public void setEdat(T edat) {
		this.edat = edat;
	}

	public String toString() {
		return "Nom: " + nom + ", Cognom: " + cognom + ", Edat: " + edat + "";
	}
	
	//Metode propi de la classe generic
	public <T> String imprimirDades (T nom, T cognom, T edat ) {
		return "Nom: " + nom + ", Cognom: " + cognom + ", Edat: " + edat + "";
	}
	
}
