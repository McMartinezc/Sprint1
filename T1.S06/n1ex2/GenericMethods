package n1ex2;


public class GenericMethods <T> { //T,U,K declaración para clases genericas
	
	//Declaració de variables
	private T nom;
	private T cognom;
	private T edat;
	
	//Metode constructor
	public GenericMethods(T dada, T dada2, T valor) {
		nom = dada;
		cognom= dada2;
		edat = valor;
	}
	
	//Metodes GETTER i SETTER
	public T getNom() {
		return nom;
	}

	public void setNom(T dada) {
		nom = dada;
	}

	public T getCognom() {
		return cognom;
	}

	public void setCognom(T dada2) {
		cognom = dada2;
	}

	public T getEdat() {
		return edat;
	}

	public void setEdat(T valor) {
		edat = valor;
	}

	//Metode que printa
	public <T>String  printarDades() {
		return "GenericMethods [nom=" + nom + ", cognom=" + cognom + ", edat=" + edat + "]";
	}

		
}
