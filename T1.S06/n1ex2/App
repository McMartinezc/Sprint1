package n1ex2;

public class App {
	
	
	public static void main(String[] args) {

		GenericMethods <Persona> pers1 = new GenericMethods <Persona> (new Persona("MC", "Martinez",37), new Persona("MC", "Martinez",37), new Persona("MC", "Martinez",37));
		GenericMethods <String> pers2 = new GenericMethods <String>("MC", "Martinez"," 37");
		GenericMethods <Integer> pers3 = new GenericMethods <Integer>(40, 37, 37);
		
		
		System.out.println(pers1.printarDades());
		System.out.println(pers2.printarDades());
		System.out.println(pers3.printarDades());
		
	}
	
}
