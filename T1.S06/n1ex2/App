package n1ex2;

public class App {
	
	
	public static void main(String[] args) {

		GenericMethods <Persona> pers1 = new GenericMethods <Persona>(null, null, null);
		GenericMethods <String> pers2 = new GenericMethods <String>(null, null, null);
		GenericMethods <Integer> pers3 = new GenericMethods <Integer>(null, null, null);
		
		
		
		System.out.println(pers1.imprimirDades("Mc", "Mart√≠nez", 37)); //Imprimim persona1 tipus classe Persona
		System.out.println(pers2.imprimirDades("Garcia", 40 , "David")); //Imprimim persona2 tipus classe String
		System.out.println(pers3.imprimirDades(50, 60, 180)); //Imprimim persona3 tipus Integer
	}
	
}
