package n2ex1;

public class Smartphone extends Telefon implements Camara, Rellotge {

			
	//Metode constructor	
	public Smartphone (String marca, String model) {
		super (marca,model);
	}
	
	//Metode propi marcat per la interficie Camara
	public String fotografiar() {
		return "S'està fent una foto" ;
	}
	
	//Metode propi marcat per la interficie rellotge
	public String alarma() {
		return "Està sonant l'alarma";
	}
}
